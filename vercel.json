{
  "functions": {
    "api/**": {
      "excludeFiles": "{.next,.git,node_modules}/**"
    }
  },
  "buildCommand": "next build --no-lint",
  "ignoreCommand": "echo 'Skipping lint check'",
  "installCommand": "pnpm install",
  "framework": "nextjs",
  "env": {
    "NEXT_DISABLE_ESLINT": "1",
    "NEXT_TELEMETRY_DISABLED": "1"
  }
}
